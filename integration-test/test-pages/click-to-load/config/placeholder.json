{
  "unprotectedTemporary": [],
  "features": {
    "clickToLoad": {
      "state": "enabled",
      "exceptions": [
        {
          "domain": "duckduckgo.com",
          "reason": "Warnings are already displayed before embedded YouTube videos are loaded."
        }
      ],
      "settings": {
        "Facebook, Inc.": {
          "ruleActions": [
            "block-ctl-fb"
          ],
          "state": "enabled"
        },
        "Youtube": {
          "ruleActions": [
            "block-ctl-yt"
          ],
          "state": "disabled"
        }
      }
    }
  }
}
